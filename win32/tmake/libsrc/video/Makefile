####### Implicit rules

.SUFFIXES: .cpp .cxx .cc .c

.cpp.obj:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -Fo$@ $<

.cxx.obj:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -Fo$@ $<

.cc.obj:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -Fo$@ $<

.c.obj:
	$(CC) -c $(CFLAGS) $(INCPATH) -Fo$@ $<

####### Build rules

all: $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LIB) /OUT:$(TARGET) @<<
	    $(OBJECTS) $(OBJMOC)
<<

moc: $(SRCMOC)

tmake: Makefile

Makefile: video.pro
	tmake video.pro -o Makefile

dist:
	$(ZIP) video.zip video.pro $(SOURCES) $(HEADERS) $(DIST) $(INTERFACES)

clean:
	-del video_dispatch.obj
	-del im_video_v4l1.obj
	-del $(TARGET)

